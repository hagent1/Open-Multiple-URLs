{
  "name": "Open Multiple URLs",
  "version": "1.5",
  "manifest_version": 2,
  "description": "Opens a list of URLs",
  "homepage_url" : "https://github.com/htrinter/Open-Multiple-URLs",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon19.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Insert"
      },
      "description": "Open the menu to insert URLs."
    }
  }
}
